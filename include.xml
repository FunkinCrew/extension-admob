<?xml version="1.0" encoding="utf-8"?>
<extension>
    <section if="android">
        <dependency name="extension-admobex" path="dependencies/extension-admobex" />
    </section>

    <section if="ios">
        <ndll name="admobex" />

        <dependency path="frameworks/GoogleMobileAds.framework" />
